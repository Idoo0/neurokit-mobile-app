<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/warmup_navigation"
    app:startDestination="@id/warmupStartFragment">

    <fragment
        android:id="@+id/warmupStartFragment"
        android:name="com.example.neurokit.ui.warmup.WarmupStartFragment"
        android:label="Warmup Start"
        tools:layout="@layout/fragment_warmup_start">
        <action
            android:id="@+id/action_warmupStartFragment_to_warmupQuestionFragment"
            app:destination="@id/warmupQuestionFragment" />
    </fragment>

    <fragment
        android:id="@+id/warmupQuestionFragment"
        android:name="com.example.neurokit.ui.warmup.WarmupQuestionFragment"
        android:label="Warmup Question"
        tools:layout="@layout/fragment_warmup_question">

        <action
            android:id="@+id/action_warmupQuestionFragment_to_warmupResultFragment"
            app:destination="@id/warmupResultFragment" />

        <action
            android:id="@+id/action_warmupQuestionFragment_to_warmupEndFragment"
            app:destination="@id/warmupEndFragment" />
    </fragment>

    <fragment
        android:id="@+id/warmupResultFragment"
        android:name="com.example.neurokit.ui.warmup.WarmupResultFragment"
        android:label="Warmup Result"
        tools:layout="@layout/fragment_warmup_result" >
        <action
            android:id="@+id/action_warmupResultFragment_to_warmupQuestionFragment"
            app:destination="@id/warmupQuestionFragment"
            app:popUpTo="@id/warmupQuestionFragment"
            app:popUpToInclusive="true"/>
    </fragment>

    <fragment
        android:id="@+id/warmupEndFragment"
        android:name="com.example.neurokit.ui.warmup.WarmupEndFragment"
        android:label="Warmup End"
        tools:layout="@layout/fragment_warmup_end" >
        <action
            android:id="@+id/action_warmupEndFragment_to_warmupStartFragment"
            app:destination="@id/warmupStartFragment"
            app:popUpTo="@id/warmupStartFragment"
            app:popUpToInclusive="true" />
    </fragment>

</navigation>